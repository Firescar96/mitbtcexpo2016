<!DOCTYPE html>
<html lang="en">
<head>
  <!-- META DATA -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
  <meta name="description" content="MIT Bitcoin Expo 2017">
  <meta http-equiv="cache-control" content="no-cache, must-revalidate, post-check=0, pre-check=0" />
    <meta http-equiv="cache-control" content="max-age=0" />
    <meta http-equiv="expires" content="0" />
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT" />
    <meta http-equiv="pragma" content="no-cache" />

  <title>MIT Bitcoin Expo 2018</title>

  <!-- STYLESHEETS -->
  <link rel="stylesheet" href="../assets/style/bootstrap.min.css" type="text/css" />
  <link rel="stylesheet" href="../assets/style/index.css?v=1.6" type="text/css" />
  <link rel="icon" type="image/x-icon" href="../assets/images/ico/favicon.ico">

  <!-- GOOGLE WEB FONTS -->
  <link href='http://fonts.googleapis.com/css?family=Lato' rel='stylesheet' type='text/css'>
</head>

<body>
  <!-- NAVIGATION -->
  <nav id="navigation" class="navbar navbar-inverse dropdown down">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="/">MIT BITCOIN EXPO</a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div id="navbar" class="navbar-collapse collapse" aria-expanded="false">
      <ul class="nav navbar-nav">
        <li><font color=#ffc40d><a href="../index.html">2019 Expo</a></font></li>
      </ul>
    </div>
  </nav>
  <!-- END NAVIGATION -->

  <main id="main">
    <!-- PAGE | WELCOME -->
    <div class="pages white page-welcome fullheight flashback" id="page-welcome">
      <div class="center">
        <div class="line-divider" id="banner">
          <h1>!=$</h1>
        </div>
        <h2>MIT Bitcoin Expo 2018</h2>
        <h3>March 17-18</h3>

        <div class="social-icons sicon-white bordered">
          <a href="https://www.facebook.com/MITBitcoinClub" class="sicon-facebook"><i>Facebook</i></a>
          <a href="https://twitter.com/mitbitcoinclub" class="sicon-twitter"><i>Twitter</i></a>
          <a href="https://www.youtube.com/user/MITBitcoinClub" class="sicon-youtube"><i>Youtube</i></a>
        </div>
      </div>
    </div>
    <!-- END PAGE | Welcome -->

    <!-- PAGE | COMPETITION -->
    <div class="pages page-competition" id="page-competition">
      <div class="center">
        <div class="container">
          <header id="schedule">
            <div class="line-divider">
              <h1>Highlights</h1>
            </div>
          </header>
          <div class="aspect-ratio">
            <iframe src="https://www.youtube.com/embed/d26r0YDeqq4" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
          </div>
        </div>
      </div>
    </div>
    <!-- END PAGE | COMPETITION -->

    <!-- PAGE | SCHEDULE -->
    <div class="pages white page-schedule" id="page-schedule">
      <div class="container">
        <header id="schedule">
          <h1 class="line-divider">Events</h1>
        </header>
        <h2>Talks</h2>
        <table class="table table-condensed col-md-8">
          <thead>
            <tr>
              <th><h4>Topic</h4></th>
              <th><h4>Speakers</h4></th>
            </tr>
          </thead>
          <tbody>
            {% for index, [time,event,format,speaker] in enumerate(day1_26) %}
            <tr>
              <td>{{event}}</td>
              <td>{{speaker}}</td>
            </tr>
            {% end %}
          </tbody>
        </table>
        <h2>Panels and Competitions</h2>
        <table class="table table-condensed">
          <thead>
            <tr>
              <th><h4>Topic</h4></th>
              <th><h4>Moderator</h4></th>
              <th><h4>Panelists</h4></th>
            </tr>
          </thead>
          <tbody>
            {% for index, [time,format,event,moderator,panelists] in enumerate(day1_32) %}
            <tr>
              <td>{{event}}</td>
              <td>{{moderator}}</td>
              <td>{{panelists}}</td>
            </tr>
            {% end %}
          </tbody>
        </table>
    <!-- END PAGE | SCHEDULE -->

    <!-- PAGE | SPONSORS -->
    <div class="pages page-sponsors" id="page-sponsors">
      <div class="container">
        <header id="schedule">
          <div class="line-divider">
            <h1>Sponsored By</h1>
          </div>
        </header>
        <div class="row-fluid">
          <div class="center">
            <h4>PLATINUM LEVEL</h4>
            {% for index, (image,name,url) in enumerate(platinumsponsors) %}
            <a href="{{url}}"><img class="sponsor-platinum" src=".{{image}}" alt="{{name}}"></img></a>
            <br/><br/>
            {% end %}
            </br></br>
            <h3>GOLD LEVEL</h3>
            {% for index, (image,name,url) in enumerate(goldsponsors) %}
            <a href="{{url}}"><img class="sponsor-gold" src=".{{image}}" alt="{{name}}"></img></a>
            {% end %}
            </br></br>
             <h3>SILVER LEVEL</h3>
             {% for index, (image,name,url) in enumerate(silversponsors) %}
             <a href="{{url}}"><img class="sponsor-silver" src=".{{image}}" alt="{{name}}"></img></a>
             {% end %}
             </br></br>
           <h3>ACADEMIC SPONSOR</h3>
           {% for index, (image,name,url) in enumerate(academicsponsors) %}
           <a href="{{url}}"><img class="sponsor-academic" src=".{{image}}" alt="{{name}}"></img></a>
           {% end %}
           <br/><br/>
          </div>
        </div>
      </div>
      <div id="map-image">
        <img src="../assets/images/pages/contact/bck.jpg">
      </div>
    </div>
    <!-- END PAGE | SPONSORS -->

  </main>

  <!-- JQUERY -->
  <script src="assets/js/jquery-2.2.4.min.js" type="text/javascript"></script>

  <!-- TWITTER BOOTSTRAP -->
  <script src="assets/js/bootstrap.min.js" type="text/javascript"></script>

  <!-- PLUGINS -->
  <script src="assets/js/plugins/jquery.fixedonlater.min.js" type="text/javascript"></script>
  <script src="assets/js/plugins/scrollspy.js" type="text/javascript"></script>

  <!-- INITIALIZE -->
  <script src="assets/js/application.js" type="text/javascript"></script>

</body>

</html>
